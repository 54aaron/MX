int main() {
    int i;
    float f;

    i = 9 + -8;
    print(i);

    i = 8 * (9 / 2);
    print(i);

    i = 3 / 2 + 1;
    print(i);

    f = 4.5 + 6;  # casting
    printf(f);

    f = 3 * 7.89;
    printf(f);  

    f = 11.0 / -2.75;
    printf(f);

    i = -1 / 3;
    print(i);

    i = -1  * -3;
    print(i);

    i = 1  * -3;
    print(i);

    f = -1.0 / -4.0;
    printf(f);

    f = 1.0 / -4.0;
    printf(f);
    
    f = 1.0 / 4.0;
    printf(f);

    f = -1.0 + -4.0;
    printf(f);
}
